  <head data-hook="inside_head">
    <%= render :partial => 'spree/shared/head' %>
    
  </head>

  <body class="<%= body_class %>" id="<%= @body_id || 'default' %>" data-hook="body">
    <div class="admisales">
    <div class="container">

      <header id="header" class="row" data-hook>
      

        <!-- begin #header -->
        <div id="header" class="clearfix">
        
        <%#*<ul class="navigation navsimple">%>
          
          <%#*<li class=" cc1"><a href="http://localhost:3000/"><cufon style="width: 39px; height: 14px;" alt="Home" class="cufon cufon-canvas"><canvas style="width: 45px; height: 14px; top: 0px; left: -1px;" height="14" width="45"></canvas><cufontext></cufontext></cufon></a></li>  <a href="http://localhost:3000/cart"><cufon style="width: 39px; height: 14px;"class="cufon cufon-canvas"><canvas style="width: 45px; height: 14px; top: 0px; left: -1px;" height="14" width="45"></canvas><cufontext><li id="link-to-cart" data-hook><%= link_to_cart %></li></cufontext></cufon></a>%>
     
        <%#*</ul>%>
          <%#*<li id="home-link" data-hook><%= link_to t(:home), root_path %>
          <%#*<li id="link-to-cart" data-hook><%= link_to_cart %>

       </div>
<!-- end #header -->

        <figure id="logo" class="columns alpha six" data-hook>
          &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= logo %>
        </figure>

        <nav id="top-nav-bar" class="columns omega ten">
          <ul id="nav-bar" class="inline" data-hook>
            <%= render :partial => 'spree/shared/nav_bar' %>
       
          </ul>
        </nav>

        <%# if store_menu? %>
          <%#*<nav class="columns alpha omega sixteen">%>
            <%#*<ul id="main-nav-bar" class="inline" data-hook>%>
              <%#= render :partial => 'spree/shared/store_menu' %>
            <%#*</ul>%>
          <%#*</nav>%>
        <%# end %>

      </header>

      <% if store_menu? %>
          <nav class="columns alpha omega sixteen">
            <ul id="main-nav-bar" class="inline" data-hook>
              <%= render :partial => 'spree/shared/store_menu' %>

       
            </ul>
          </nav>
        <% end %>

    

      <div id="wrapper" class="row" data-hook>

        <% if content_for?(:sidebar) %>
          <aside id="sidebar" class="columns alpha four" data-hook>
            <%= yield :sidebar %>
          </aside>
        <% end %>

        <div id="content" class="columns omega <% if !content_for?(:sidebar) %> sixteen alpha <% else %> twelve <% end %>" data-hook>
          <%= breadcrumbs(@taxon) %>
          <%= flash_messages %>
          <%= yield %>
        </div>

      </div>

      <footer id="footer" class="alpha omega sixteen columns" data-hook>
        <div id="footer-left" class="columns alpha eight" data-hook>
          <p><%= t(:powered_by) %> <%= link_to 'NewTech Fusion Solutions', 'http://www.newtechfusion.com/' %></p>
        </div>
        <div id="footer-right" class="columns omega eight" data-hook></div>
      </footer>

    </div>
    </div>

    <%= render :partial => 'spree/shared/google_analytics' %>

  </body>

</html>