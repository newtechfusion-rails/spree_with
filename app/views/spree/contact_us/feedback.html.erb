
<%= form_for(@feedback, :as =>:feed, :url => "/contactus/feedback", :method =>"post") do |f| %>

<h1>Want to give your opinion, kindly do so below!</h1>

<label>User Name</label>
<%=  f.text_field :user_name %>

<label>Email-id </label>
<%=  f.email_field :email_id %>

<label>Feedback regarding</label>
<%=  f.text_field :feedback_regarding %>

<label>Feedback</label>
<%=  f.text_area :feedback %>

<%= f.submit "Submit Feedback" %>

<%end%>